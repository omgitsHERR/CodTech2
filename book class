import java.util.*;

//Book class
class Book {
    String title;
    String author
    int year;
    int price;
    public Book(String title, String author, int year,int price) {
        this.title = title;
        this.author = author;
        this.year = year;
        this.price=price;
    }
    public String getTitle() {
        return title;
    }
    public String getAuthor() {
        return author;
    }
    public int getYear() {
        return year;
    }
}
